generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Place {
  id        String       @id @default(auto()) @map("_id") @db.ObjectId
  type      TypeOfPlaces
  name      String       @unique
  city      String
  zipcode   Int
  country   String
  updatedAt DateTime     @updatedAt
}

enum TypeOfPlaces {
  bar
  museum_pay
  museum_free
  restaurant
  monument
}
